{
  "format_version": "1.8.0", 
  "render_controllers": {
	"controller.render.chicken": {
	  "part_visibility": [
		{ "comb0": "!query.is_baby" },
		{ "comb1": "!query.is_baby" }
	  ],
	  "geometry": "geometry.default", 
	  "materials": [
	    { "*": "Material.default" }, 
	    { "leg*": "Material.legs" }
	  ], 
	  "textures": [ "query.is_baby ? Texture.default_baby : Texture.default" ]
    }
  }
}
